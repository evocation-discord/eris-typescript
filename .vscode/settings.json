{
  "eslint.validate": [
    "javascript",
    "typescript",
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.tabSize": 2,
  "search.exclude": {
    "**/node_modules": true,
    "**/*.code-search": true,
    "yarn.lock": true
  },
  "npm.packageManager": "yarn",
  "eslint.packageManager": "yarn",
  "typescript.tsdk": "node_modules/typescript/lib",
  "files.eol": "\n"
}